<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.innvideo.renderpoc">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".gles.EditorActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gles.LearningActivity">
        </activity>
        <activity android:name=".poc.FramesExtractorActivity" />
        <activity android:name=".poc.HopeItCombinesActivity"></activity>
        <activity android:name=".praveen.PraveenActivity" />
        <activity android:name=".TrialActivity" />
        <activity android:name=".GLActivity" />
        <activity android:name=".BasicActivity" />
        <activity android:name=".SurfaceActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>